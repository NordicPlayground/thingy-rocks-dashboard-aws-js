{
  "name": "@nordicsemiconductor/thingy-rocks-dashboard",
  "version": "0.0.0-development",
  "description": "thingy.rocks AWS IoT dashboard developed using Preact in TypeScript",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicPlayground/thingy-rocks-dashboard-aws-js.git"
  },
  "keywords": [
    "thingy-world",
    "iot",
    "aws",
    "demo",
    "preact"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicPlayground/thingy-rocks-dashboard-aws-js/issues"
  },
  "homepage": "https://github.com/NordicPlayground/thingy-rocks-dashboard-aws-js#readme",
  "scripts": {
    "start": "vite",
    "build": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build --emptyOutDir",
    "prepare": "husky install",
    "postinstall": "cp -r node_modules/svg-country-flags/svg/ static/flags",
    "test": "jest"
  },
  "dependencies": {
    "@aws-amplify/core": "5.8.5",
    "@aws-sdk/credential-provider-cognito-identity": "3.427.0",
    "@nordicsemiconductor/rsrp-bar": "6.0.0",
    "@sentry/react": "7.73.0",
    "@sentry/tracing": "7.73.0",
    "compare-versions": "6.0.0-rc.1",
    "date-fns": "2.30.0",
    "e118-iin-list": "3.1.0",
    "lodash-es": "4.17.21",
    "lucide-preact": "0.284.0",
    "maplibre-gl": "3.4.0",
    "preact": "10.18.1",
    "preact-router": "4.1.2",
    "styled-components": "6.0.8",
    "svg-country-flags": "1.2.10"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.94",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "16.0.22",
    "@nordicsemiconductor/from-env": "2.0.0",
    "@preact/preset-vite": "2.5.0",
    "@swc/core": "1.3.92",
    "@swc/jest": "0.2.29",
    "@types/jest": "29.5.5",
    "@types/lodash-es": "4.17.9",
    "@types/node": "20.8.4",
    "@types/styled-components": "5.1.28",
    "bootstrap": "5.3.2",
    "chalk": "5.3.0",
    "eslint": "8.51.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-unicorn": "48.0.1",
    "handlebars": "4.7.8",
    "husky": "8.0.3",
    "id128": "1.6.6",
    "jest": "29.7.0",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "vite": "4.4.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "jest": {
    "testRegex": ".+\\.spec\\.tsx?$",
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  }
}
